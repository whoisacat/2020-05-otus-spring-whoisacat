<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questionDao" class="com.whoisacat.edu.testingApp1.dao.QustionDaoSimple">
    </bean>
    <bean id="questionService" class="com.whoisacat.edu.testingApp1.service.QuestionService">
        <property name="dao" ref="questionDao"/>
        <property name="fileName" value="questions.csv"/>
    </bean>
</beans>
